{
  "name": "paperang-extension",
  "displayName": "paperang extension",
  "description": "",
  "keywords": [
    "paperang",
    "zuoyebang",
    "zyb"
  ],
  "publisher": "psilolau",
  "icon": "icon.png",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Paperang",
      "properties": {
        "pomodoro.focus": {
          "type": "number",
          "default": 25,
          "markdownDescription": "Minutes of pomodoro time, see more about the [Pomodoro Technique](https://en.wikipedia.org/wiki/Pomodoro_Technique)."
        },
        "pomodoro.nap": {
          "type": "number",
          "default": 5,
          "description": "Minutes of break time."
        },
        "pomodoro.visible": {
          "type": "boolean",
          "default": false,
          "description": "Whether display the counting in the status bar."
        }
      }
    },
    "commands": [
      {
        "command": "extension.startPomodoro",
        "title": "start pomodoro üçÖ"
      },
      {
        "command": "extension.resetPomodoro",
        "title": "reset pomodoro üçÖ"
      },
      {
        "command": "extension.closePomodoro",
        "title": "close pomodoro üçÖ"
      }
    ],
    "snippets": [
      {
        "language": "less",
        "path": "./snippets/paperang-theme.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.56.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "14.x",
    "eslint": "^7.19.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
